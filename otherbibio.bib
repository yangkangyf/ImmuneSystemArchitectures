@article{Buckley64,
title = "Diversity of animal immune receptors and the origins of recognition complexity in the deuterostomes",
journal = "Developmental \& Comparative Immunology ",
volume = "49",
number = "1",
pages = "179 - 189",
year = "2015",
note = "",
issn = "0145-305X",
doi = "http://dx.doi.org/10.1016/j.dci.2014.10.013",
url = "http://www.sciencedirect.com/science/article/pii/S0145305X14002602",
author = "Katherine M. Buckley and Jonathan P. Rast",
keywords = "TLR",
keywords = "NLR",
keywords = "Deuterostome",
keywords = "Sea urchin",
keywords = "Immune recognition ",
abstract = "Abstract Invertebrate animals are characterized by extraordinary diversity in terms of body plan, 
life history and life span. The past impression that invertebrate immune responses are controlled by relativel
y simple innate systems is increasingly contradicted by genomic analyses that reveal significant evolutionary 
novelty and complexity. One accessible measure of this complexity is the multiplicity of genes encoding homolo
gs of pattern recognition receptors. These multigene families vary significantly in size, and their sequence c
haracter suggests that they vary in function. At the same time, certain aspects of downstream signaling appear
 to be conserved. Here, we analyze five major classes of immune recognition receptors from newly available ani
mal genome sequences. These include the Toll-like receptors (TLR), Nod-like receptors (NLR), \{SRCR\} domain s
cavenger receptors, peptidoglycan recognition proteins (PGRP), and Gram negative binding proteins (GNBP). We d
iscuss innate immune complexity in the invertebrate deuterostomes, which was first recognized in sea urchins, 
within the wider context of emerging genomic information across animal phyla. "
}

