@article{Barclay:2003,
title = "Membrane proteins with immunoglobulin-like domains—a master superfamily of interaction molecules",
journal = "Seminars in Immunology",
volume = "15",
number = "4",
pages = "215 - 223",
year = "2003",
note = "Genomics and Immunology",
issn = "1044-5323",
doi = "https://doi.org/10.1016/S1044-5323(03)00047-2",
url = "http://www.sciencedirect.com/science/article/pii/S1044532303000472",
author = "A.Neil Barclay",
keywords = "Domain, Immunoglobulin-like, Interaction, Membrane, Protein",
abstract = "Recent genomic analysis has shown that proteins with immunoglobulin superfamily (IgSF) domains are extremely abundant and their number has increased markedly in evolution correlating with the development of the adaptive immune system. The IgSF domain is particularly good at being recognised and is involved in many different kinds of interactions. Thus, analysis of the properties of these domains can act as a paradigm for thinking about the roles of newly identified gene products. This review summarises the identification, function and properties of IgSF domains including, their size, variety of interactions, their strength of binding, role of glycosylation and organisation with other proteins."
}
