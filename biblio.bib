	Issn = {0022-2836},
	Journal = {Journal of Molecular Biology},
	Keywords = {protein structure prediction},
	Number = {3},
	Pages = {567 - 580},
	Title = {Predicting transmembrane protein topology with a hidden markov model: application to complete genomes1},
